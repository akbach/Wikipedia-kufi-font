{
    "name": "Wikipedia kufi font",
    "version": "0.0.1",
    "manifest_version": 2,
    "description": "Read Wikipedia with Google droid arabic kufi font - مطالعة ويكبيديا بإستعمل خط درويد كوفي",
    "homepage_url": "http://rtl.tips",
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "content_scripts": [{
        "matches": [
            "https://ar.wikipedia.org/*",
            "http://ar.wikipedia.org/*"
        ],
        "css": [
            "css/inject.css"
        ]
    }]
}
